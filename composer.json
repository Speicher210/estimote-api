{
  "name": "speicher210/estimote-api",
  "type": "library",
  "description": "Estimote cloud API",
  "keywords": [
    "estimote",
    "estimote cloud",
    "api"
  ],
  "homepage": "https://github.com/Speicher210/estimote-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Dragos Protung",
      "email": "dragos@protung.ro",
      "role": "Developer"
    }
