    "guzzlehttp/guzzle": "^6.2",
    "jms/serializer": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "scrutinizer/ocular": "^1.3",
    "squizlabs/php_codesniffer": "^2.8",
    "doctrine/cache": "^1.6"
